{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_main.py",
      "use": "@vercel/python",
      "config": {
        "maxLambdaSize": "50mb",
        "runtime": "python3.9"
      }
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel_main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/website/index.html"
    }
  ]
}
