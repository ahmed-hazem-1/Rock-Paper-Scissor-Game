{
  "functions": {
    "api/main.py": {
      "runtime": "python3.9"
    }
  },
  "builds": [
    {
      "src": "api/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/website/index.html"
    }
  ]
}
