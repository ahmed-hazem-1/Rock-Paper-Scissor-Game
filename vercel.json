{
  "version": 2,
  "builds": [
    {
      "src": "api/vercel_main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/api/vercel_main.py"
    },
    {
      "src": "/(.*)",
      "dest": "/website/index.html"
    }
  ],
  "functions": {
    "api/vercel_main.py": {
      "maxDuration": 30,
      "memory": 1024
    }
  },
  "github": {
    "silent": true
  }
}
